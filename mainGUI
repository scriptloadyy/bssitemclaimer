-- Scripts

local function YGSBUSJ_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)

	local frame = script.Parent
	
	local locationsTbl = {
		["1"] = Vector3.new(110.860512, 63.7177925, -59.5777168),
		["2"] = Vector3.new(-293.512756, 50.1799393, 266.900116),
		["3"] = Vector3.new(-357.538208, 129.899857, -227.214569),
		["4"] = Vector3.new(139.456604, 64.4310455, 258.481812),
		["5"] = Vector3.new(142.535141, 69.5271149, -217.049393),
		["6"] = Vector3.new(6.46148396, 228.992645, -169.161346),
		["7"] = Vector3.new(-444.322754, 122.515938, 353.599243),
