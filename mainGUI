		["55"] = Vector3.new(-54.3523827, 19.4243584, -62.2778854),
		["56"] = Vector3.new(34.8642769, 57.9650383, 190.510757),
		["57"] = Vector3.new(-122.991501, 67.3708878, -218.436462),
		["58"] = Vector3.new(83.8066254, 69.4766388, -142.149338),
		["59"] = Vector3.new(263.946198, 57.1383095, 108.368507),
		["60"] = Vector3.new(600.636108, 177.167313, -436.898132),
		["61"] = Vector3.new(499.400879, 177.16832, -420.62561),
		["62"] = Vector3.new(518.912781, 177.167679, -291.619812),
		["63"] = Vector3.new(524.506042, 151.901627, -411.875977),
		["64"] = Vector3.new(48.2289505, 31.3100128, 705.013367),
	
	}
	
	local plr = game.Players.LocalPlayer
	local frame = script.Parent
	
	-- Function to get HRP reliably
	local function getHRP()
		local char = plr.Character or plr.CharacterAdded:Wait()
		return char:WaitForChild("HumanoidRootPart")
	end
	
	-- Teleport function
	local function goTo(pos)
		local hrp = getHRP()
		hrp.CFrame = CFrame.new(pos) + Vector3.new(math.random(-2,2), math.random(0,1), math.random(-2,2))
	end
	
	-- Create buttons
	for i, pos in pairs(locationsTbl) do
		local btn = script.Button:Clone()
		btn.Parent = frame.buttonsFolder
		btn.Name = tostring(i)
		btn.Text = "Go to " .. i
	
		-- Add a Vector3Value if not already
		local valueObj = btn:FindFirstChild("Value")
		if not valueObj then
			valueObj = Instance.new("Vector3Value")
			valueObj.Name = "Value"
			valueObj.Parent = btn
		end
		valueObj.Value = pos
	
		btn.Position = UDim2.new(0, 15, 0, i * 55)
	
		-- Connect click here, not later
		btn.Activated:Connect(function()
			goTo(valueObj.Value)
		end)
	end
	
	local plr = game.Players.LocalPlayer
	local frame = script.Parent
	
	local function setupCharacterButtons(char)
		local humanoid = char:WaitForChild("Humanoid")
	
		-- Dance button
		local danceButton = frame:WaitForChild("dance")
		local danceAnim = Instance.new("Animation")
		danceAnim.AnimationId = "rbxassetid://135373056067761"
		local danceTrack = humanoid:LoadAnimation(danceAnim)
	
		danceButton.Activated:Connect(function()
			if danceTrack.IsPlaying then
				danceTrack:Stop()
			end
			danceTrack:Play()
		end)
	
		-- Trip button
		local tripButton = frame:WaitForChild("trip")
		tripButton.Activated:Connect(function()
			humanoid:ChangeState(Enum.HumanoidStateType.Physics)
			delay(3, function()
				if humanoid.Health > 0 then
					humanoid:ChangeState(Enum.HumanoidStateType.GettingUp) end end)
		end)
	
		-- Sit button
		local sitButton = frame:WaitForChild("sit")
		sitButton.Activated:Connect(function()
			humanoid.Sit = true
		end)
	end
	
	-- Initial setup
	if plr.Character then
		setupCharacterButtons(plr.Character)
	end
	
	-- Reconnect buttons every respawn
	plr.CharacterAdded:Connect(function(char)
		setupCharacterButtons(char)
	end)
	
	
	local UserInputService = game:GetService("UserInputService")
	local plr = game.Players.LocalPlayer
	local objective = script.Parent.Parent
	
	-- Optional: start hidden
	objective.Visible = false
	
	-- Toggle function
	local function toggleObjective()
		objective.Visible = not objective.Visible
	end
	
	-- Listen for key press
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end -- ignore if the player is typing in a chat
		if input.UserInputType == Enum.UserInputType.Keyboard then
			if input.KeyCode == Enum.KeyCode.F then
				toggleObjective()
			end
		end
	end)
	
end
coroutine.wrap(YGSBUSJ_fake_script)()
local function VIGNW_fake_script() -- openclose.LocalScript 
	local script = Instance.new('LocalScript', openclose)

	local objective = script.Parent.Parent.ItemTPer
	
	script.Parent.Activated:Connect(function()
		if objective.Visible == true then
			objective.Visible = false
		else
			objective.Visible = true
		end
	end)
end
coroutine.wrap(VIGNW_fake_script)()
